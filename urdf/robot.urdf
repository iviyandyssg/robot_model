    <gazebo reference="ultrasonic_front_link">
        <sensor name="ultrasonic_sensor_front" type="ray">
            <pose>0.2 0 0.05 0 0 0</pose>
            <!-- 是否可视化，gazebo里能不能看到 -->
            <visualize>true</visualize>
            <update_rate>10</update_rate>
            <ray>
                <scan>
                    <!-- 水平扫描的点数 -->
                    <horizontal>
                        <samples>10</samples>
                        <resolution>1</resolution>
                        <min_angle>-0.131</min_angle>
                        <max_angle>0.131</max_angle>
                    </horizontal>
                </scan>

                <!-- 超声波检测的范围和数据分辨率单位m -->
                <range>
                    <min>0.1</min>
                    <max>3.00</max>
                    <resolution>0.01</resolution>
                </range>

                <noise>
                    <type>gaussian</type>
                    <mean>0.0</mean>
                    <stddev>0.01</stddev>
                </noise>
            </ray>

            <plugin name="ultrasonic_sensor_front_c" filename="libgazebo_ros_ray_sensor.so">
                <ros>
                    <remapping>~/out:=ultrasonic_front</remapping>
                </ros>
                <output_type>sensor_msgs/Range</output_type>
                <radiation_type>ultrasound</radiation_type>
                <frame_name>ultrasonic_front_link</frame_name>
            </plugin>
        </sensor>
    </gazebo>

